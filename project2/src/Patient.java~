

public class Patient{
	public:
		String firstName, middleName, lastName;
		String address, city, state, ZIP;
		String phoneNum;
		String EMcontact, EMphoneNum;
			
	Patient(){

	}

	Patient(String firstName, String middleName, String lastName){
		this.firstName = firstName;
		this.middleName = middleName;
		this.lastName = lastName;
	}
	
	Patient(String firstName, String middleName, String lastName, String phoneNum, String EMcontact, String EMphoneNum)){
		this(firstName, middleName, lastName);	
		this.phoneNum = phoneNum;
		this.EMcontact = Emcontact;
		this.EMphoneNum = EMphoneNum;
	}
	
	String buildFullName(){
		return firstName + " " + middleName + " " +  lastName;	
	}
	String buildAddress(){
		return address + " " city + " " + state + " " + ZIP;
	}

	String buildEmergencyContact(){	
		return EMcontact + " " + EMphoneNum;	
	}
	
	void patientToString(){
		System.out.println(buildFullName());
		System.out.println(buildAddress());
		System.out.println(buildEmergencyContact());
	}

	private:


}
